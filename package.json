{
  "name": "dn-react-uistring",
  "description": "DN-React-Uistring is a library for the best uistring.xml (Dragon Nest) preview.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-lib/main.js",
  "types": "dist-lib/main.d.ts",
  "files": [
    "dist-lib"
  ],
  "scripts": {
    "dev": "vite",
    "build:frontend": "tsc --p ./tsconfig.json && vite build --config vite_frontend.config.ts",
    "build:lib": "tsc --p ./tsconfig-build.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "html-react-parser": "^5.1.18"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "glob": "^11.0.0",
    "globals": "^15.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass-embedded": "^1.79.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3",
    "vite-plugin-lib-inject-css": "^2.1.1"
  }
}
